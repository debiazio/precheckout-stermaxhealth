{
  "vendor": "stermaxhealth",
  "name": "precheckout",
  "version": "0.0.2",
  "title": "precheckout",
  "description": "App para fazer uma tela de cadastro antes do carrinho",
  "builders": {
    "react": "3.x",
    "messages": "1.x",
    "docs": "0.x",
    "store": "0.x",
    "node": "6.x"
  },
  "dependencies": {
    "vtex.product-context": "0.x",
    "vtex.css-handles": "0.x",
    "vtex.search-graphql": "0.x",
    "vtex.flex-layout": "0.x",
    "vtex.render-runtime": "8.x"
  },
  "registries": [
    "smartcheckout"
  ],
  "routes": {
    "saveClient": {
      "path": "/_v/precheckout/client"
    }
  },
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/dataentities/*"
      }
    },
    {
      "name": "ADMIN_DS"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
